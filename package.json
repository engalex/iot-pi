    "del": "2.2.0",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.0",
    "gulp-clean-css": "2.0.6",
    "gulp-cssnano": "2.1.2",
    "gulp-filter": "4.0.0",
    "gulp-htmlmin": "1.3.0",
    "gulp-imagemin": "2.4.0",
    "gulp-inject": "4.0.0",
