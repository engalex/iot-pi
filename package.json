{
  "name": "iot-pi",
  "version": "1.0.0",
  "description": "the Raspberry Pi 3, IBM Watson IoT Platform, and Node micro-blog of edm00se",
  "main": "gulp",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "bundle exec jekyll build --config _config.yml,_localPreview.yml",
